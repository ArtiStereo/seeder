{
    "name": "artistereo/yii2-seeder",
    "description": "I create this extension so that everyone can create their own seeder factory",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","seeder"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "ArtiStereo",
            "email": "artistereo69@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "artistereo\\seeder\\": ""
        }
    },
    "controllerMap": {
        "seeder": {
            "class": "artistereo\\seeder\\console\\SeederController",
        },
    },
    "repositories":[
        {
            "type": "git",
            "url": "https://github.com/ArtiStereo/seeder"
        }
    ]
}
